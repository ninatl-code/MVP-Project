# Stripe OAuth Configuration
# Get these values from https://dashboard.stripe.com/developers/api-keys
EXPO_PUBLIC_STRIPE_OAUTH_CLIENT_ID=YOUR_STRIPE_OAUTH_CLIENT_ID
EXPO_PUBLIC_STRIPE_OAUTH_REDIRECT_URI=https://yourdomain.com/stripe-callback

# Google OAuth Configuration
# Get these values from https://console.cloud.google.com
EXPO_PUBLIC_GOOGLE_OAUTH_CLIENT_ID=YOUR_GOOGLE_OAUTH_CLIENT_ID
EXPO_PUBLIC_GOOGLE_OAUTH_REDIRECT_URI=https://yourdomain.com/google-callback

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=YOUR_SUPABASE_URL
EXPO_PUBLIC_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY

# App Configuration
EXPO_PUBLIC_API_URL=https://yourdomain.com/api
EXPO_PUBLIC_ENVIRONMENT=production

# Deep Links
EXPO_PUBLIC_DEEP_LINK_PREFIX=monappmobile://
